{
    "name": "santosdave/travelport",
    "description": "PHP client library for Travelport SOAP API",
    "license": "MIT",
    "authors": [
        {
            "name": "Santos Dave",
            "email": "santosdave86@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/santosdave/travelport"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^9.19|^10|^11"
    },
    "autoload": {
        "psr-4": {
            "Santosdave\\Travelport\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Santosdave\\Travelport\\TravelportServiceProvider"
            ],
            "aliases": {
                "Travelport": "Santosdave\\Travelport\\TravelportFacade"
            }
        }
    }
}
